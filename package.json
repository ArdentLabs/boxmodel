{
  "name": "boxmodel",
  "version": "3.0.2",
  "description": "API to abstract models, generating common functionality",
  "main": "dist/boxmodel.umd.js",
  "module": "dist/boxmodel.es5.js",
  "browser": "dist/boxmodel.iife.js",
  "typings": "index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ArdentLabs/gradebook-api"
  },
  "scripts": {
    "start": "mocha -w --watch-extensions ts -r ts-node/register src/**/*.spec.ts",
    "build": "npm run clean && mkdirp dist && npm-run-all --parallel build:development build:production",
    "build:development": "NODE_ENV=development rollup -c",
    "build:production": "NODE_ENV=production rollup -c",
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint-fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "test": "mocha -r ts-node/register src/**/*.spec.ts"
  },
  "author": "Sam Balana <sam.balana@ardentacademy.com>",
  "license": "ISC",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "deepmerge": "^1.5.1",
    "normalizr": "^3.2.3",
    "pluralize": "^7.0.0",
    "react": "^15.6.1",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.6",
    "reselect": "^3.0.1",
    "sift-sort": "^1.0.0"
  },
  "devDependencies": {
    "@types/deepmerge": "^1.3.1",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.0.26",
    "@types/pluralize": "^0.0.27",
    "@types/react": "^16.0.5",
    "@types/react-router": "^4.0.15",
    "@types/react-router-redux": "^5.0.7",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "cz-conventional-changelog": "^2.0.0",
    "mocha": "^3.5.0",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.49.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-filesize": "^1.4.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript2": "^0.5.2",
    "rollup-plugin-uglify": "^2.0.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2",
    "uglify-es": "^3.0.28"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
